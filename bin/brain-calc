#!/usr/bin/env php
<?php  

$autoload_path_v1 = '../vendor/autoload.php';
$autoload_path_v2 = __DIR__ . "/../../../autoload.php";
$autoload_path_v3 = __DIR__ . "\\..\\..\\..\\autoload.php";

if (file_exists($autoload_path_v1)) {
    require_once $autoload_path_v1;
} elseif (file_exists($autoload_path_v2)) {
    require_once $autoload_path_v2;
} else {
    require_once $autoload_path_v3;
}

use function BrainGames\Games\calc\run;

run();

?>